public class Armor extends Item
{  
  //Defense
  private int defense;
   /**
   * Method to get defense
   * @return defense
   */
  public int getDefense()
  {
    return defense;
  }
  /**
   * constructor to initialize defense
   * @param defense
   */
  public void setDefense (int defense)
  {
    this.defense = defense;
  }
}
