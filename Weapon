public class Weapon extends Item
{  
  private int damage;
    /**
    * Method to get the damage of item
    * @return the damage
    */
  public int getDamage()
  {
    return damage;
  }
  /**
   * constructor to initialize the damage
   * @param damage
   */
  public void setDamage (int damage)
  {
    this.damage = damage;
  }
}
